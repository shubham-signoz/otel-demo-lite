# Copy this file to .env and fill in your values
# cp .env.example .env

# Your SigNoz ingestion url (from Settings > Ingestion in SigNoz UI)
# Self-hosted: your-host:4317
OTLP_ENDPOINT=https://ingest.us.staging.signoz.cloud:443

# Your SigNoz ingestion key (from Settings > Ingestion in SigNoz UI)
# Not needed for self-hosted
SIGNOZ_INGESTION_KEY=

# Set to true for self-hosted without TLS (plain gRPC on port 4317)
OTLP_INSECURE=false

# Set to true to skip TLS certificate verification (for self-signed certs)
OTLP_INSECURE_SKIP_VERIFY=false

# Number of simulated orders (0 = run as servers only for load testing)
COUNT=5
